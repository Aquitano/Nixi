{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Extension",
  "extends": "./base.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "noEmit": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "types": [
      "chrome",
      "vite/client"
    ]
  }
}
