{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Extension",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "ESNext",
    "noEmit": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "types": [
      "chrome",
      "vite/client"
    ],
    "useDefineForClassFields": true
  }
}
