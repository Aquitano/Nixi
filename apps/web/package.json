{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue,css,scss}\" --config ../../.prettierrc",
    "lint": "eslint \"src/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}\" --fix"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "supertokens-auth-react": "^0.27.0",
    "supertokens-web-js": "^0.2.2",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^16.11.56",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "config": "workspace:*",
    "eslint": "^8.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tsconfig": "workspace:*",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  }
}
