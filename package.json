{
	"name": "nixi",
	"version": "0.0.1",
	"license": "GPL-3.0",
	"author": "Thomas Aquitano Breindl (https://aquitano.github.io/)",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build --no-daemon",
		"dev": "turbo run dev --parallel --no-daemon",
		"lint": "turbo run lint --no-daemon",
		"format": "turbo run format --no-daemon",
		"prepare": "husky install"
	},
	"devDependencies": {
		"husky": "^8.0.3",
		"turbo": "^1.10.9"
	},
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.7.0"
}
