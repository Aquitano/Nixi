{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prisma:dev:deploy": "prisma migrate deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:dev:rm": "docker compose rm db -s -f -v",
    "db:dev:up": "docker compose up db -d",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:dev:restart": "pnpm run db:dev:rm && pnpm run db:dev:up && sleep 1 && pnpm run prisma:dev:deploy",
    "db:test:restart": "pnpm run db:test:rm && pnpm run db:test:up && sleep 1 && pnpm run prisma:test:deploy",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue,css,scss}\" --config ../../.prettierrc",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}\" --fix",
    "test": "jest",
    "pretest:e2e": "pnpm run db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@prisma/client": "^4.2.1",
    "argon2": "^0.29.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.2",
    "@nestjs/testing": "^9.0.11",
    "@types/express": "^4.17.13",
    "@types/jest": "29.0.2",
    "@types/node": "^18.7.15",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.0.1",
    "jest": "^29.0.1",
    "pactum": "^3.1.14",
    "prettier": "^2.3.2",
    "prisma": "^4.2.1",
    "source-map-support": "^0.5.20",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.3.5",
    "config": "workspace:*"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
